    "Data_Event Codeblock":{
        "prefix": "DATA_EVENT",
        "body": [
            "DATA_EVENT[${1:device}]",
            "{",
            "\tONLINE:",
            "\t{",
            "\t\t$0",
            "\t}",
            "\tOFFLINE: { }",
            "\tCOMMAND: { }",
            "\tSTRING:",
            "\t{",
            "\t\t",
            "\t}",
            "}"
        ],
        "description": "Basic DATA_EVENT block"
    },
